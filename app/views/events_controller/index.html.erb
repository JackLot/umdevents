<h3>Contents of ICal file</h3>

<% @cal.first.events.each do |event| %>
	<%= event.summary %> | <%= event.dtstart %> <br />
<% end %>

<hr />

<%= @firstEvent.summary %>


<h1>Listing All Events</h1>

<table>

	<% events = Event.all %>

	<% events.each do |event| %>
		<tr>
			<td><%= event.name %> |</td>
			<td><%= event.organization %></td>
		</tr>
	<% end %>

</table>

<% if events.size == 0 %>
	<em>No events found</em>

<% end %>